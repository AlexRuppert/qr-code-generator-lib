{
  "name": "qr-code-generator-lib",
  "version": "0.1.0",
  "description": "A tiny qr code generation library",
  "main": "./dist/qr-code-generator-lib.es.js",
  "author": "Alexander Ruppert",
  "license": "MIT",
  "private": false,
  "type": "module",
  "module": "./dist/qr-code-generator-lib.es.js",
  "types": "dist/types/index.d.ts",
  "devDependencies": {
    "gh-pages": "^4.0.0",
    "typescript": "^4.7.4",
    "vite": "^2.9.12",
    "vitest": "^0.15.1"
  },
  "scripts": {
    "build": "vite build && tsc index.ts --declaration --downlevelIteration --emitDeclarationOnly --esModuleInterop --outDir dist/types",
    "dev": "vite dev --config vite.config.demo.ts",
    "demo": "vite build --config vite.config.demo.ts",
    "test": "vitest",
    "types": "tsc index.ts --declaration --downlevelIteration --emitDeclarationOnly --esModuleInterop --outDir dist/types",
    "gh": "node publish.js"
  },
  "exports": {
    "require": "./dist/qr-code-generator-lib.cjs.js",
    "default": "./dist/qr-code-generator-lib.es.js"
  },
  "files": [
    "dist",
    "src"
  ],
  "keywords": [
    "qr",
    "qrcode"
  ]
}
